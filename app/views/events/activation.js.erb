$('#active').bind('ajax:success', function () {
    $("#active").html('Закрыть регистрацию');
    $('#active').removeClass('btn btn-sm btn-inline-secondary').addClass('btn btn-sm btn-inline-secondary');
    console.log("good");
});


$('#inactive').bind('ajax:success', function () {
    $("#inactive").html('Открыть регистрацию');
    $('#inactive').removeClass('btn btn-sm btn-inline-secondary').addClass('btn btn-sm btn-inline-secondary');
    console.log("good");
});
